            <!-- home bar -->
            <div class="partners">
                <div
                    class="container xl:max-w-7xl mx-auto text-center lg:px-20"
                >
                    <div
                        class="grid grid-cols-2 py-5 md:py-0 md:grid-cols-4 gap-3 md:gap-10"
                    >
                        <div
                            class="partner px-5 py-1 md:py-10 flex items-center justify-center"
                        >
                            <a>
                                <img
                                    src="images/gco-logo.svg"
                                    alt="Gco group"
                                />
                            </a>
                        </div>
                        <div
